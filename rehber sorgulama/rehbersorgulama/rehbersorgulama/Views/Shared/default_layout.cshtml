<!DOCTYPE html>
<html lang="tr">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Title -->
    <title>Home - Andtourtravel </title>
    <!-- Bootstrap css -->
    <link rel="stylesheet" href="/Content/css/bootstrap.min.css" />
    <!-- animate css -->
    <link rel="stylesheet" href="/Content/css/animate.min.css" />
    <!-- Fontawesome css -->
    <link rel="stylesheet" href="/Content/css/fontawesome.all.min.css" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.8.2/font/bootstrap-icons.css">
    <!-- owl.carousel css -->
    <link rel="stylesheet" href="/Content/css/owl.carousel.min.css" />

    <!-- owl.theme.default css -->
    <link rel="stylesheet" href="/Content/css/owl.theme.default.min.css" />
    <!-- navber css -->
    <link rel="stylesheet" href="/Content/css/navber.css" />
    <!-- meanmenu css -->
    <link rel="stylesheet" href="/Content/css/meanmenu.css" />
    <!-- Style css -->
    <link rel="stylesheet" href="/Content/css/style.css" />
    <!-- Responsive css -->
    <link rel="stylesheet" href="/Content/css/responsive.css" />
    <!-- Favicon -->
    <link rel="icon" type="image/png" href="/Content/img/favicon.png">
</head>
<style>
    :root {
        --main-color: #E5771A;
        --white-color: #ffffff;
        --black-color: #2B2540;
        --black-color-opacity: #2b2540c4;
        --paragraph-color: #818090;
        --bg-color: #F3F6FD;
        --transition: .4s all ease-in-out;
    }


    a.gflag {
        vertical-align: middle;
        font-size: 32px;
        padding: 1px 0;
        margin-left: 20px;
    }

    a.gflag img {
        border: 0;
    }



    #goog-gt-tt {
        display: none !important;
    }

    .goog-te-banner-frame {
        display: none !important;
    }

    .goog-te-menu-value:hover {
        text-decoration: none !important;
    }

    body {
        top: 0 !important;
    }

    #google_translate_element2 {
        display: none !important;
    }

</style>
<body>
    @Html.Partial("~/Views/Partial/_header.cshtml")
    @RenderBody()
    @Html.Partial("~/Views/Partial/_footer.cshtml")

    <script src="/Content/js/jquery-3.6.0.min.js"></script>
    <!-- Bootstrap js -->
    <script src="/Content/js/bootstrap.bundle.js"></script>
